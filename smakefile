OBJECTS=petrobots.o Platform.o PlatformAmiga.o PlatformAmigaAssembler.o Palette.o PT2.3F_replay_cia.o C64Font.o Faces.o Tiles.o Sprites.o SpritesMask.o Items.o Health.o SoundFXModule.o SoundExplosion.o SoundMedkit.o SoundMagnet.o SoundShock.o SoundMove.o SoundPlasma.o SoundPistol.o SoundError.o SoundCycleWeapon.o SoundCycleItem.o SoundDoor.o SoundMenuBeep.o SquareWave.o

all: petrobots 

petrobots: $(OBJECTS)
   sc link to petrobots with <<
$(OBJECtS)
<

petrobots.o: petrobots.cpp
Platform.o: Platform.cpp
PlatformAmiga.o: PlatformAmiga.cpp
PlatformAmigaAssembler.o: PlatformAmigaAssembler.s
Palette.o: Palette.cpp
PT2.3F_replay_cia.o: PT2.3F_replay_cia.s
C64Font.o: C64Font.raw
    Bin2Hunk C64Font.raw C64Font.o NAME _c64Font HUNK data
Faces.o: Faces.raw
    Bin2Hunk Faces.raw Faces.o NAME _facesPlanes CHIP
Tiles.o: Tiles.raw
    Bin2Hunk Tiles.raw Tiles.o NAME _tilesPlanes CHIP
Sprites.o: Sprites.raw
    Bin2Hunk Sprites.raw Sprites.o NAME _spritesPlanes CHIP
SpritesMask.o: SpritesMask.raw
    Bin2Hunk SpritesMask.raw SpritesMask.o NAME _spritesMask CHIP
Items.o: Items.raw
    Bin2Hunk Items.raw Items.o NAME _itemsPlanes CHIP
Health.o: Health.raw
    Bin2Hunk Health.raw Health.o NAME _healthPlanes CHIP
SoundFXModule.o: mod.soundfx
    Bin2Hunk mod.soundfx SoundFXModule.o NAME _soundFXModule CHIP
SoundExplosion.o: sounds_dsbarexp.raw
    Bin2Hunk sounds_dsbarexp.raw SoundExplosion.o NAME _soundExplosion CHIP
SoundMedkit.o: SOUND_MEDKIT.raw
    Bin2Hunk SOUND_MEDKIT.raw SoundMedkit.o NAME _soundMedkit CHIP
SoundMagnet.o: SOUND_MAGNET.raw
    Bin2Hunk SOUND_MAGNET.raw SoundMagnet.o NAME _soundMagnet CHIP
SoundShock.o: SOUND_SHOCK.raw
    Bin2Hunk SOUND_SHOCK.raw SoundShock.o NAME _soundShock CHIP
SoundMove.o: SOUND_MOVE.raw
    Bin2Hunk SOUND_MOVE.raw SoundMove.o NAME _soundMove CHIP
SoundPlasma.o: SOUND_PLASMA.raw
    Bin2Hunk SOUND_PLASMA.raw SoundPlasma.o NAME _soundPlasma CHIP
SoundPistol.o: sounds_dspistol.raw
    Bin2Hunk sounds_dspistol.raw SoundPistol.o NAME _soundPistol CHIP
SoundError.o: SOUND_ERROR.raw
    Bin2Hunk SOUND_ERROR.raw SoundError.o NAME _soundError CHIP
SoundCycleWeapon.o: SOUND_CYCLE_WEAPON.raw
    Bin2Hunk SOUND_CYCLE_WEAPON.raw SoundCycleWeapon.o NAME _soundCycleWeapon CHIP
SoundCycleItem.o: SOUND_CYCLE_ITEM.raw
    Bin2Hunk SOUND_CYCLE_ITEM.raw SoundCycleItem.o NAME _soundCycleItem CHIP
SoundDoor.o: SOUND_DOOR.raw
    Bin2Hunk SOUND_DOOR.raw SoundDoor.o NAME _soundDoor CHIP
SoundMenuBeep.o: SOUND_BEEP.raw
    Bin2Hunk SOUND_BEEP.raw SoundMenuBeep.o NAME _soundMenuBeep CHIP
SquareWave.o: SquareWave.raw
    Bin2Hunk SquareWave.raw SquareWave.o NAME _squareWave CHIP

clean:
    delete $(OBJECTS) petrobots petrobots.info petrobots.lnk
