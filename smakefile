OBJECTS=petrobots.o Platform.o PlatformAmiga.o PlatformAmigaAssembler.o Palette.o PT2.3F_replay_cia.o IntroScreen.o GameScreen.o C64Font.o Tiles.o Sprites.o SpritesMask.o Items.o Health.o SoundFXModule.o SoundExplosion.o SoundMedkit.o SoundPlasma.o SoundPistol.o SoundError.o SoundCycleWeapon.o SoundCycleItem.o SoundDoor.o SoundMenuBeep.o SquareWave.o

all: petrobots 

petrobots: $(OBJECTS)
   sc link to petrobots with <<
$(OBJECtS)
<

petrobots.o: petrobots.cpp
Platform.o: Platform.cpp
PlatformAmiga.o: PlatformAmiga.cpp
PlatformAmigaAssembler.o: PlatformAmigaAssembler.s
Palette.o: Palette.cpp
PT2.3F_replay_cia.o: PT2.3F_replay_cia.s
IntroScreen.o: IntroScreen.raw
    Bin2Hunk IntroScreen.raw IntroScreen.o NAME _introScreen HUNK data
GameScreen.o: GameScreen.raw
    Bin2Hunk GameScreen.raw GameScreen.o NAME _gameScreen HUNK data
C64Font.o: C64Font.raw
    Bin2Hunk C64Font.raw C64Font.o NAME _c64Font HUNK data
Tiles.o: Tiles.raw
    Bin2Hunk Tiles.raw Tiles.o NAME _tilesPlanes CHIP
Sprites.o: Sprites.raw
    Bin2Hunk Sprites.raw Sprites.o NAME _spritesPlanes CHIP
SpritesMask.o: SpritesMask.raw
    Bin2Hunk SpritesMask.raw SpritesMask.o NAME _spritesMask CHIP
Items.o: Items.raw
    Bin2Hunk Items.raw Items.o NAME _itemsPlanes CHIP
Health.o: Health.raw
    Bin2Hunk Health.raw Health.o NAME _healthPlanes CHIP
SoundFXModule.o: mod.soundfx
    Bin2Hunk mod.soundfx SoundFXModule.o NAME _soundFXModule CHIP
SoundExplosion.o: sounds_dsbarexp.raw
    Bin2Hunk sounds_dsbarexp.raw SoundExplosion.o NAME _soundExplosion CHIP
SoundMedkit.o: sounds_dsoof.raw
    Bin2Hunk sounds_dsoof.raw SoundMedkit.o NAME _soundMedkit CHIP
SoundPlasma.o: sounds_dsplasma.raw
    Bin2Hunk sounds_dsplasma.raw SoundPlasma.o NAME _soundPlasma CHIP
SoundPistol.o: sounds_dspistol.raw
    Bin2Hunk sounds_dspistol.raw SoundPistol.o NAME _soundPistol CHIP
SoundError.o: sounds_dsgetpow.raw
    Bin2Hunk sounds_dsgetpow.raw SoundError.o NAME _soundError CHIP
SoundCycleWeapon.o: sounds_dswpnup.raw
    Bin2Hunk sounds_dswpnup.raw SoundCycleWeapon.o NAME _soundCycleWeapon CHIP
SoundCycleItem.o: sounds_dsitemup.raw
    Bin2Hunk sounds_dsitemup.raw SoundCycleItem.o NAME _soundCycleItem CHIP
SoundDoor.o: sounds_dsdoropn.raw
    Bin2Hunk sounds_dsdoropn.raw SoundDoor.o NAME _soundDoor CHIP
SoundMenuBeep.o: sounds_dsswtchx.raw
    Bin2Hunk sounds_dsswtchx.raw SoundMenuBeep.o NAME _soundMenuBeep CHIP
SquareWave.o: SquareWave.raw
    Bin2Hunk SquareWave.raw SquareWave.o NAME _squareWave CHIP

clean:
    delete $(OBJECTS) petrobots petrobots.info petrobots.lnk
